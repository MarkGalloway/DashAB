const out = std_output();

procedure main() returns integer {
    integer vector i[4] = [1, 2, 3, 4];
    character vector c[4] = ['c', 'a', 't', 's'];
    real vector r[4] = [1.1, 2.2, 3.3, 4.4];
    boolean vector b[4] = [true, false, true, false];
    
    i -> out;
    c -> out; '\n' -> out;
    r -> out;
    b -> out;
    
    return 0;
}