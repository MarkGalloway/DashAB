// Strings can be constructed in the same way as vectors using character literals.
const out = std_output();

procedure main() returns integer {
	integer matrix M = [[1, 2, 3], [4, 5, 6]];

	M[1, 1] -> out;
	M[1, 2] -> out;
	M[1, 3] -> out;
	M[2, 1] -> out;
	M[2, 2] -> out;
	M[2, 3] -> out;
	
	return 0;
}