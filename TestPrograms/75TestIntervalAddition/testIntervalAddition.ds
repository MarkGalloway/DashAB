// 8.5.4 Operations: [a, b] + [c, d] = [a+c, b+d]

const out = std_output();

procedure main() returns integer {
	integer interval a = 1..5;
	integer interval b = 1..5;
	integer interval z = a + b;
	integer vector c = z; // 2..10

	c[1] -> out; '\n' -> out;	
	c[2] -> out; '\n' -> out;	
	c[3] -> out; '\n' -> out;	
	c[4] -> out; '\n' -> out;	
	c[5] -> out; '\n' -> out;	
	c[6] -> out; '\n' -> out;	
	c[7] -> out; '\n' -> out;	
	c[8] -> out; '\n' -> out;
	c[9] -> out; '\n' -> out;		
	
	return 0;
}