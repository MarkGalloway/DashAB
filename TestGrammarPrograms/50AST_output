(PROGRAM (PROCEDURE_DECL integer times_two (ARG_DECL const integer x) (Return (EXPR (* 2 x)))) (PROCEDURE_DECL integer f (Return (EXPR 1))) (PROCEDURE_DECL change_first (ARG_DECL var (tuple (FIELD_DECL var integer) (FIELD_DECL var integer)) t) (BLOCK (= (EXPR (. t 1)) (EXPR 7)))) (PROCEDURE_DECL increment (ARG_DECL var integer x) (BLOCK (= (EXPR x) (EXPR (+ x 1))))) (PROCEDURE_DECL integer main (BLOCK (VAR_DECL var (tuple (FIELD_DECL var integer) (FIELD_DECL var integer)) myTup (EXPR (TUPLE_LIST (EXPR 5) (EXPR 5)))) (VAR_DECL var integer x (EXPR 13)) (EXPR (CALL change_first (ELIST (EXPR myTup)))) (EXPR (CALL increment (ELIST (EXPR x)))) (return (EXPR 0)))))