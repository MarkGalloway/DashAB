(PROGRAM (DECL_OUTSTREAM const out std_output()) (FUNCTION_DECL integer f (Return (EXPR 1))) (FUNCTION_DECL real pythag (ARG_DECL const real a) (ARG_DECL const real b) (Return (EXPR (^ (+ (^ a 2) (^ b 2)) 0.5)))) (PROCEDURE_DECL integer main (BLOCK (PRINT out (EXPR (CALL f ELIST))) (PRINT out (EXPR '\n')) (PRINT out (EXPR (CALL pythag (ELIST (EXPR 2.0) (EXPR 2.0))))) (PRINT out (EXPR '\n')) (return (EXPR 0)))))