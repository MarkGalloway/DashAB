(PROGRAM (FUNCTION_DECL integer times_two (ARG_DECL const integer x)) (FUNCTION_DECL integer f) (FUNCTION_DECL real pythag (ARG_DECL const real a) (ARG_DECL const real b)) (PROCEDURE_DECL real get (ARG_DECL const real a) (ARG_DECL const integer i)) (PROCEDURE_DECL integer g (ARG_DECL const boolean b)) (PROCEDURE_DECL real get2 (ARG_DECL var real a) (ARG_DECL var integer i)) (PROCEDURE_DECL g2 (ARG_DECL const boolean b)) (FUNCTION_DECL integer times_two (ARG_DECL const integer x) (Return (EXPR (* 2 x)))) (FUNCTION_DECL integer f (Return (EXPR 1))) (FUNCTION_DECL real pythag (ARG_DECL const real a) (ARG_DECL const real b) (Return (EXPR (^ (+ (^ a 2) (^ b 2)) (/ 1 2))))) (PROCEDURE_DECL real get (ARG_DECL const real a) (ARG_DECL const integer i) (Return (EXPR (+ a i)))) (PROCEDURE_DECL real get2 (ARG_DECL var real a) (ARG_DECL var integer i) (Return (EXPR (+ a i)))) (PROCEDURE_DECL integer g (ARG_DECL const boolean b) (BLOCK (if (EXPR b) (BLOCK (return (EXPR 3))) (BLOCK (return (EXPR 8)))))) (PROCEDURE_DECL g2 (ARG_DECL const boolean b) (BLOCK (VAR_DECL var integer x (EXPR 1)) (if (EXPR b) (BLOCK (VAR_DECL var integer x (EXPR 1)))))))