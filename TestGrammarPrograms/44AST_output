(PROGRAM (VAR_DECL const real r (EXPR 1.0)) (VAR_DECL const integer x (EXPR (TYPECAST integer (EXPR r)))) (VAR_DECL const real r2 (EXPR (TYPECAST real (EXPR 42)))) (VAR_DECL const character c (EXPR 'a')) (VAR_DECL const boolean b (EXPR (TYPECAST boolean (EXPR c)))) (if (EXPR (!= (TYPECAST integer (EXPR b)) 0)) (BLOCK (VAR_DECL var character c2 (EXPR (TYPECAST character (EXPR 40)))))))