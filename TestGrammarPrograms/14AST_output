(PROGRAM (TYPEDEF (tuple (FIELD_DECL var real) (FIELD_DECL var real)) point) (TYPEDEF (tuple (FIELD_DECL var integer) (FIELD_DECL var real) (FIELD_DECL var bool)) bla) (TYPEDEF integer int) (TYPEDEF boolean bool) (TYPEDEF character char) (TYPEDEF real double) (BLOCK (VAR_DECL var double d1) (VAR_DECL var double d2 (EXPR 42.0)) (VAR_DECL var char c1) (VAR_DECL var char c2 (EXPR 'a')) (VAR_DECL var int x) (VAR_DECL var int y (EXPR (+ 1 2))) (VAR_DECL var bool b1) (VAR_DECL var bool b2 (EXPR false)) (VAR_DECL var point xy) (VAR_DECL var point uv (EXPR (TUPLE_LIST (EXPR 1.0) (EXPR 2.5))))))