(PROGRAM (VAR_DECL Var integer i) (VAR_DECL Var integer x (EXPR 300_000)) (VAR_DECL var integer y) (VAR_DECL var integer z (EXPR 20__)) (VAR_DECL var z (EXPR 200)) (VAR_DECL const integer a) (VAR_DECL const integer b (EXPR (- 21))) (VAR_DECL const c (EXPR 3)) (VAR_DECL Var boolean b1) (VAR_DECL Var boolean b2 (EXPR true)) (VAR_DECL var boolean b3 (EXPR false)) (VAR_DECL const boolean b4 (EXPR false)) (VAR_DECL Var character c1) (VAR_DECL Var character c2 (EXPR 'a')) (VAR_DECL var character c3 (EXPR 'b')) (VAR_DECL const character c4 (EXPR 'c')) (VAR_DECL Var real r1) (VAR_DECL Var real r2 (EXPR .42)) (VAR_DECL var real r3 (EXPR 42.e+10)) (VAR_DECL const real r4 (EXPR 42e-10)))