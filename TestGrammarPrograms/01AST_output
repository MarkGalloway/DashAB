(PROGRAM (BLOCK (VAR_DECL var integer i) (VAR_DECL var integer x (EXPR 300_000))) (VAR_DECL const integer y) (VAR_DECL const integer z (EXPR 20__)) (VAR_DECL const z (EXPR 200)) (VAR_DECL const integer a) (VAR_DECL const integer b (EXPR (- 21))) (VAR_DECL const c (EXPR 3)) (BLOCK (VAR_DECL var boolean b1) (VAR_DECL var boolean b2 (EXPR true))) (VAR_DECL const boolean b3 (EXPR false)) (VAR_DECL const boolean b4 (EXPR false)) (BLOCK (VAR_DECL var character c1) (VAR_DECL var character c2 (EXPR 'a'))) (VAR_DECL const character c3 (EXPR 'b')) (VAR_DECL const character c4 (EXPR 'c')) (BLOCK (VAR_DECL var real r1) (VAR_DECL var real r2 (EXPR .42))) (VAR_DECL const real r3 (EXPR 42.e+10)) (VAR_DECL const real r4 (EXPR 42e-10)))